<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="mech C/C++"
	>
  <Tool
		Name="VCCLCompilerTool"
		AdditionalOptions="/w44302 /w44286"
		AdditionalIncludeDirectories=".;
    $(SRC);
    $(BOOST);
    $(CLIENT_SRC);
    $(CLIENT_TOOL);
    $(LIB);
    $(LIB)\mech\src\base;
    $(LIB)\wxWidgets-2.8.12\include;
    $(LIB)\wxWidgets-2.8.12\include\msvc;
    $(LIB)\DxSDK_June2010\include;
    $(LIB)\Lua\include;
    $(LIB)\Lua\Lua_Tinker;
    $(LIB)\ProudNet\include;
    $(LIB)\DxSDK_October2006\Include;
    $(LIB)\NProtect\include;
    $(COMMON);
    $(COMMON)\PNetCommon;
    $(COMMON)\PNetClient;
    $(COMMON)\EngineJBAudio\;
    $(COMMON)\RS_Monitor;
    $(GFXSDK)\src;
    $(GFXSDK)\include;
    $(RS_COMMON);
    $(RS_COMMON)\Etc;
    $(RS_COMMON)\JBAudio;
    $(RS_COMMON)\JBAudio\JB;
    $(JELLYX_10);
    $(JELLYX_10)\JellyXApp\include;
    $(JELLYX_10)\JellyXEffect\DX9\include;
    $(JELLYX_10)\JellyXEffect\DX11\include;
    $(JELLYX_10)\JellyXMain\Include;
    $(JELLYX_10)\JellyXMain\Include\XNA;
    $(JELLYX_PLUGINS)\Plugin_DirectWrite\Include;
    $(JELLYX_PLUGINS)\Plugin_DX9RenderSystem\Include;
    $(JELLYX_PLUGINS)\Plugin_DX11RenderSystem\Include;
    $(JELLYX_PLUGINS)\Plugin_DXInput\Include;
    $(JELLYX_PLUGINS)\Plugin_OctreeSceneManager\Include;
    $(JELLYX_ANITOOL)\AnimationClassManager\include;
    $(JELLYX_TOOLS)\Tool_Item\ItemManager\include;
    "
		ForceConformanceInForLoopScope="true"
		PreprocessorDefinitions="DEF_SERVICE_TEST;DEF_COUNTRY_DEV;DEF_CLIENT_TEAM;_CRT_SECURE_NO_WARNINGS;_CRT_NON_CONFORMING_SWPRINTFS;_CRT_SECURE_NO_DEPRECATE;_CRT_NON_CONFORMING_SWPRINTFS"
		RuntimeTypeInfo="false"
		WarningLevel="3"
	/>  
  <Tool
		Name="VCLinkerTool"
		AdditionalDependencies="ws2_32.lib"
		AdditionalLibraryDirectories="
    $(TEMP_LIB);
    $(BOOST)\$(PlatformName);
    $(LIB);
    $(LIB)\LIB;
    $(LIB)\LIB\$(PlatformName);
    $(LIB)\wxWidgets-2.8.12\lib\vc_lib;
    $(LIB)\ProudNet\lib\win32;
    $(LIB)\PNet\;
    $(LIB)\DxSDK_June2010\lib\x86;
    $(LIB)\NProtect\lib;
    $(LIB)\RSLIB;
    $(LIB)\RSMonitor;    
    $(VSInstallDir);
    $(VCInstallDir)lib;
    $(VCInstallDir)atlmfc\lib;
    $(VCInstallDir)atlmfc\lib\i386;
    $(FrameworkSDKDir)lib;"
	/>
  <UserMacro
		Name="ROOT"
		Value="$(SolutionDir)..\.."
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="SRC"
		Value="$(ROOT)\src"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="LIB"
		Value="$(ROOT)\lib"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
  Name="BOOST"
  Value="$(LIB)\mech\lib\boost\boost_1_45_0"
  InheritsFromParent="true"
  Delimiter=";"
  PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="CLIENT_BASE"
		Value="$(SRC)\Client"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="CLIENT_SRC"
		Value="$(CLIENT_BASE)\Main"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="SERVER_BASE"
		Value="$(SRC)\server"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="SERVER_SRC"
		Value="$(SERVER_BASE)\Servers"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="CLIENT_TOOL"
		Value="$(CLIENT_BASE)\Tool"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="SERVER_TOOL"
		Value="$(SERVER_BASE)\Tool"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="SERVER_LIB"
		Value="$(SERVER_BASE)\ServerLib"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="COMMON"
		Value="$(SRC)\Common"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="RS_COMMON"
		Value="$(SRC)\Common\RS_Common"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="BIN"
		Value="$(ROOT)\bin\DEV\TEST\Client"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="TOOL"
		Value="$(ROOT)\bin\DEV\TEST\Tool"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
    Name="RSLIB_NAME"
    Value="$(ROOT)\lib\RSLIB\$(ProjectName)_$(ConfigurationName)_$(PlatformName)"
    InheritsFromParent="true"
    Delimiter=";"
    PerformEnvironmentSet="true"
	/>
  <UserMacro
    Name="PNETLIB_NAME"
    Value="$(ROOT)\lib\PNet\$(ProjectName)_$(ConfigurationName)_$(PlatformName)"
    InheritsFromParent="true"
    Delimiter=";"
    PerformEnvironmentSet="true"
	/>  
  <UserMacro
		Name="BIN_NAME"
		Value="$(ROOT)\bin\DEV\TEST\Client\$(ProjectName)_$(ConfigurationName)_$(PlatformName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="TOOL_NAME"
		Value="$(ROOT)\bin\DEV\TEST\Tool\$(ProjectName)_$(ConfigurationName)_$(PlatformName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="OUT"
		Value="$(ROOT)\obj\DEV\TEST\Client\$(ProjectName)\$(ConfigurationName)_$(PlatformName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
		Name="TEMP_LIB"
		Value="$(ROOT)\obj\DEV\TEST\Client\$(ConfigurationName)_$(PlatformName)"
		InheritsFromParent="true"
		Delimiter=";"
		PerformEnvironmentSet="true"
	/>
  <UserMacro
    Name="JELLYX_ENGINE"
    Value="$(SRC)\Engine_ver1.0"
    InheritsFromParent="true"
    Delimiter=";"
    PerformEnvironmentSet="true"
	/>
  <UserMacro
    Name="JELLYX_10"
    Value="$(JELLYX_ENGINE)\JellyX_1.0"
    InheritsFromParent="true"
    Delimiter=";"
    PerformEnvironmentSet="true"
	/>
  <UserMacro
    Name="JELLYX_Plugins"
    Value="$(JELLYX_10)\Plugins"
    InheritsFromParent="true"
    Delimiter=";"
    PerformEnvironmentSet="true"
	/>
  <UserMacro
    Name="JELLYX_TOOLS"
    Value="$(JELLYX_ENGINE)\Tools"
    InheritsFromParent="true"
    Delimiter=";"
    PerformEnvironmentSet="true"
	/>
  <UserMacro
    Name="JELLYX_ANITOOL"
    Value="$(JELLYX_TOOLS)\Tool_AniView"
    InheritsFromParent="true"
    Delimiter=";"
    PerformEnvironmentSet="true"
	/>
</VisualStudioPropertySheet>



